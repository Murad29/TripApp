<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:TripApp"
                    xmlns:v="clr-namespace:TripApp.Views"
                    xmlns:vm="clr-namespace:TripApp.ViewModels">
    <DataTemplate DataType="{x:Type vm:TripListViewModel}">
        <v:TripListView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:AddEditTripViewModel}">
        <v:AddEditTripView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:CityViewModel}">
        <v:CitiesView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:MenuViewModel}">
        <v:MenuView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:AddEditTicketViewModel}">
        <v:AddEditTicketView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:TicketListViewModel}">
        <v:TicketListView/>
    </DataTemplate>
</ResourceDictionary>